{
    "name": "nestjs-graphql-starter",
    "description": "A NestJS GraphQL starter project with comprehensive setup",
    "version": "1.0.0",
    "license": "MIT",
    "author": "hmake98",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "dev": "npm-run-all --parallel studio start:dev",
        "start": "node dist/main",
        "start:dev": "nest start --watch",
        "typesync": "npx typesync",
        "update:packages": "npx npm-check-updates -u",
        "seed:user": "nestjs-command seed:user",
        "generate": "prisma generate",
        "migrate": "prisma migrate dev",
        "migrate:prod": "prisma migrate deploy",
        "studio": "prisma studio",
        "lint": "eslint \"{src,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "test": "jest --config test/jest.json --runInBand --passWithNoTests --forceExit",
        "prepare": "husky"
    },
    "engines": {
        "node": ">=20.0.0",
        "yarn": ">=1.22.0"
    },
    "dependencies": {
        "@apollo/server": "4.11.0",
        "@aws-sdk/client-s3": "^3.645.0",
        "@aws-sdk/client-sts": "^3.645.0",
        "@aws-sdk/credential-providers": "^3.645.0",
        "@aws-sdk/s3-request-presigner": "^3.645.0",
        "@nestjs/apollo": "12.2.0",
        "@nestjs/common": "10.4.1",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "10.4.1",
        "@nestjs/graphql": "12.2.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "10.4.1",
        "@nestjs/terminus": "^10.2.3",
        "@nestjs/throttler": "^6.2.1",
        "@prisma/client": "^5.20.0",
        "argon2": "^0.41.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "compression": "^1.7.4",
        "express": "^4.19.2",
        "graphql": "^16.9.0",
        "graphql-subscriptions": "2.0.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "nestjs-command": "^3.1.4",
        "nestjs-i18n": "^10.4.5",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "reflect-metadata": "0.2.2",
        "rxjs": "7.8.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.4.1",
        "@commitlint/config-conventional": "^19.4.1",
        "@nestjs/cli": "10.4.5",
        "@nestjs/schematics": "10.1.4",
        "@nestjs/testing": "10.4.1",
        "@types/compression": "~1.7.5",
        "@types/eslint": "~9.6.1",
        "@types/eslint-config-prettier": "~6.11.3",
        "@types/express": "^4.17.21",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "~9.0.6",
        "@types/lint-staged": "~13.3.0",
        "@types/lodash": "~4.17.7",
        "@types/node": "22.5.2",
        "@types/passport": "~1.0.16",
        "@types/passport-jwt": "~4.0.1",
        "@types/supertest": "6.0.2",
        "@typescript-eslint/eslint-plugin": "8.4.0",
        "@typescript-eslint/parser": "8.4.0",
        "eslint": "^8.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-jest": "^28.8.2",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.1.0",
        "husky": "^9.1.5",
        "jest": "29.7.0",
        "jsonc-eslint-parser": "^2.4.0",
        "lint-staged": "^15.2.10",
        "npm-run-all": "^4.1.5",
        "prettier": "3.3.3",
        "prisma": "^5.20.0",
        "rimraf": "6.0.1",
        "supertest": "7.0.0",
        "ts-jest": "29.2.5",
        "ts-loader": "9.5.1",
        "ts-morph": "23.0.0",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.5.4"
    },
    "lint-staged": {
        "{src,test}/**/*.{ts,js,json}": [
            "prettier --write",
            "eslint --fix"
        ]
    }
}
